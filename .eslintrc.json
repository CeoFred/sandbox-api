{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "require-jsdoc":"off",
        "linebreak-style":"off",
        "max-len":"warn",
        "no-unused-vars":"warn"
    }
}
